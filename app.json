{
  "expo": {
    "name": "SmokeLess",
    "slug": "SmokeLess",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#26A65B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smokeless.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#26A65B"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.smokeless.app"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-apple-authentication"
    ],
    "scheme": "smokeless",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://g5y8wrbctkd3t5cqsjp4vhbv9eg4w5y6.app.specular.dev"
    }
  },
  "scheme": "SmokeLess"
}